# Smithery API key (get from smithery.ai)
SMITHERY_API_KEY=your-smithery-api-key

# Base Sepolia RPC
RPC_URL=https://sepolia.base.org

# Wallet private key for signing transactions (with 0x prefix)
PRIVATE_KEY=0x...

# Pinata JWT for IPFS pinning (get from pinata.cloud)
PINATA_JWT=your-pinata-jwt

# The Graph API key (get from thegraph.com)
THE_GRAPH_API_KEY=your-graph-api-key

# Optional: API keys for testing MCP servers
EXA_API_KEY=your-exa-api-key
