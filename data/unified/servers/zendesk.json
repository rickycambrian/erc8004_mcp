    {
      "name": "ZENDESK_CREATE_ZENDESK_ORGANIZATION",
      "description": "Tool to create a Zendesk organization. Use when you need to group end users under a shared company profile. Use after confirming the organization name is unique.",
      "inputSchema": {
        "type": "object",
        "title": "CreateZendeskOrganizationRequest",
        "required": [
          "name"
        ],
        "properties": {
          "name": {
            "type": "string",
            "title": "Name",
            "examples": [
              "Acme Corp"
            ],
            "description": "Unique organization name. Leading/trailing whitespace is trimmed by Zendesk."
          },
          "external_id": {
            "type": "string",
            "title": "External Id",
            "default": null,
            "examples": [
              "ext-1234"
            ],
            "nullable": true,
            "description": "Optional unique external identifier for this organization. Comparison is case-insensitive; use to sync with external systems."
          }
        },
        "description": "Request parameters for creating a new Zendesk organization."
