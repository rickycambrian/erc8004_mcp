    },
    {
      "name": "ZENDESK_CREATE_ZENDESK_TICKET",
      "description": "Create a ticket in Zendesk.",
      "inputSchema": {
        "type": "object",
        "title": "CreateZendeskTicketRequest",
        "required": [
          "subject",
          "description"
        ],
        "properties": {
          "subject": {
            "type": "string",
            "title": "Subject",
            "examples": [
              "Unable to reset password",
              "Billing discrepancy"
            ],
            "description": "Short summary of the issue. Keep it concise (e.g. 'Cannot log in')."
          },
          "priority": {
            "enum": [
              "urgent",
              "high",
              "normal",
              "low"
            ],
            "type": "string",
            "title": "Priority",
            "default": "normal",
            "examples": [
              "high"
            ],
            "description": "Zendesk priority. Must be one of: 'urgent', 'high', 'normal', 'low'."
          },
          "description": {
            "type": "string",
            "title": "Description",
            "examples": [
              "User reports\u2026"
            ],
            "description": "Long-form description / steps to reproduce."
          },
          "requester_name": {
            "type": "string",
            "title": "Requester Name",
            "default": null,
            "examples": [
              "Jane Doe"
            ],
            "nullable": true,
            "description": "Name of the requester. If you supply this, you MUST also supply `requester_email`. Leave both blank to default to the authenticated user."
