{
  "id": "@Hzzy2O/flux-cloudfare-mcp",
  "name": "@Hzzy2O/flux-cloudfare-mcp",
  "display_name": "Flux Cloudflare MCP",
  "version": null,
  "description": "Generate high-quality images using AI with seamless integration into your applications. Leverage customizable parameters for image generation and enjoy secure processing through Cloudflare Workers. Enhance your AI assistants with powerful image capabilities effortlessly.",
  "icon_url": "https://icons.duckduckgo.com/ip3/github.com.ico",
