  "tools": [
    {
      "name": "search",
      "description": "Search the web using multiple engines (e.g., Baidu, Bing, DuckDuckGo, CSDN, Exa, Brave, Juejin(\u6398\u91d1)) with no API key required",
      "inputSchema": {
        "type": "object",
        "$schema": "http://json-schema.org/draft-07/schema#",
        "required": [
          "query"
        ],
        "properties": {
          "limit": {
            "type": "number",
            "default": 10,
            "maximum": 50,
            "minimum": 1
          },
          "query": {
            "type": "string",
            "minLength": 1
          },
          "engines": {
            "type": "array",
            "items": {
              "enum": [
                "baidu",
                "bing",
                "csdn",
                "duckduckgo",
                "exa",
                "brave",
                "juejin"
              ],
              "type": "string"
            },
            "default": [
              "bing"
            ],
            "minItems": 1
          }
        },
        "additionalProperties": false
      }
    },
    {
