    {
      "name": "view_terminal_history",
      "title": "View Terminal History",
      "description": "View the terminal history. Can return full history or only the latest output.",
      "inputSchema": {
        "type": "object",
        "$schema": "http://json-schema.org/draft-07/schema#",
        "required": [
          "terminal_id"
        ],
        "properties": {
          "full": {
            "type": "boolean",
            "default": true,
            "description": "If true, returns full history; otherwise only latest output"
          },
          "terminal_id": {
            "type": "string",
            "description": "Terminal identifier"
          }
        },
        "additionalProperties": false
      }
    },
    {
      "name": "kill_terminal_process",
      "title": "Kill Terminal Process",
      "description": "Kill the current process running in the specified terminal.",
      "inputSchema": {
        "type": "object",
        "$schema": "http://json-schema.org/draft-07/schema#",
        "required": [
          "terminal_id"
        ],
        "properties": {
          "terminal_id": {
            "type": "string",
            "description": "Terminal identifier"
          }
        },
        "additionalProperties": false
      }
    },
    {
      "name": "write_to_terminal",
      "title": "Write to Terminal",
      "description": "Write input to a terminal process, optionally simulating an Enter key press.",
      "inputSchema": {
        "type": "object",
        "$schema": "http://json-schema.org/draft-07/schema#",
        "required": [
          "terminal_id",
          "text"
        ],
        "properties": {
          "text": {
            "type": "string",
            "description": "Text to write to the terminal"
          },
          "enter": {
            "type": "boolean",
            "default": false,
            "description": "Whether to simulate pressing Enter after the text"
          },
          "terminal_id": {
            "type": "string",
            "description": "Terminal identifier"
          }
        },
        "additionalProperties": false
      }
    },
    {
      "name": "initialize_sandbox",
      "title": "Initialize Sandbox",
