      "inputSchema": {
        "type": "object",
        "$schema": "http://json-schema.org/draft-07/schema#",
        "required": [
          "secrets"
        ],
        "properties": {
          "secrets": {
            "type": "object",
            "description": "Key-value pairs of secrets to initialize",
            "additionalProperties": {
              "type": "string"
            }
          }
        },
        "additionalProperties": false
      }
    },
    {
      "name": "health_check",
      "title": "Health Check",
      "description": "Check the health status of the Manus sandbox service.",
      "inputSchema": {
        "type": "object",
        "$schema": "http://json-schema.org/draft-07/schema#",
        "properties": {},
        "additionalProperties": false
      }
    },
    {
      "name": "zip_and_upload_project",
      "title": "Zip and Upload Project",
      "description": "Zip a directory (excluding specified folders) and upload the archive to S3.",
      "inputSchema": {
        "type": "object",
        "$schema": "http://json-schema.org/draft-07/schema#",
        "required": [
          "directory",
          "upload_url",
          "project_type"
        ],
        "properties": {
          "directory": {
            "type": "string",
            "description": "Directory path to zip"
          },
          "upload_url": {
            "type": "string",
            "description": "Pre-signed S3 URL for upload"
          },
          "project_type": {
            "enum": [
              "frontend",
              "backend",
              "nextjs"
            ],
            "type": "string",
            "description": "Type of project being uploaded"
          }
        },
        "additionalProperties": false
      }
    }
