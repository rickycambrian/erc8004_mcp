  "tools": [
    {
      "name": "search",
      "description": "Search for products that match a query with optional filters",
      "inputSchema": {
        "type": "object",
        "$schema": "http://json-schema.org/draft-07/schema#",
        "properties": {
          "limit": {
            "anyOf": [
              {
                "type": "integer"
              },
              {
                "type": "null"
              }
            ],
            "default": 20
          },
          "query": {
            "type": [
              "string",
              "null"
            ]
          },
          "filters": {
            "anyOf": [
              {
                "type": "object",
                "properties": {
                  "price": {
                    "anyOf": [
                      {
                        "type": "object",
                        "properties": {
                          "max_price": {
                            "type": [
                              "number",
                              "null"
                            ]
                          },
                          "min_price": {
                            "type": [
                              "number",
                              "null"
                            ]
                          }
                        },
                        "additionalProperties": false
                      },
                      {
                        "type": "null"
                      }
                    ]
                  },
                  "gender": {
                    "anyOf": [
                      {
                        "enum": [
                          "male",
                          "female",
                          "unisex"
                        ],
                        "type": "string"
                      },
                      {
                        "type": "null"
                      }
                    ]
                  },
                  "brand_ids": {
                    "anyOf": [
                      {
                        "type": "array",
                        "items": {
                          "type": "string"
                        }
                      },
                      {
                        "type": "null"
                      }
                    ]
                  },
                  "availability": {
                    "anyOf": [
                      {
                        "type": "array",
                        "items": {
                          "enum": [
                            "InStock",
                            "OutOfStock",
                            "PreOrder",
                            "LimitedAvailability",
                            "BackOrder",
                            "Discontinued",
                            "SoldOut",
                            "Unknown"
                          ],
                          "type": "string"
                        }
                      },
                      {
                        "type": "null"
                      }
                    ]
                  }
                },
                "additionalProperties": false
              },
              {
                "type": "null"
              }
            ],
            "default": {}
          },
          "image_url": {
            "type": [
              "string",
              "null"
            ]
          }
        },
        "additionalProperties": false
      }
    },
    {
      "name": "get_product_detail",
      "description": "Get detailed information about a specific product by its ID",
      "inputSchema": {
        "type": "object",
        "$schema": "http://json-schema.org/draft-07/schema#",
        "required": [
          "product_id"
        ],
        "properties": {
          "product_id": {
            "type": "string"
          }
        },
        "additionalProperties": false
      }
    },
    {
      "name": "get_brands",
      "description": "Get all brands that the vendor currently sells with optional query",
      "inputSchema": {
        "type": "object",
        "$schema": "http://json-schema.org/draft-07/schema#",
        "properties": {
          "page": {
            "type": "integer",
            "default": 1
          },
          "size": {
            "type": "integer",
            "default": 100
          },
          "query": {
            "type": [
              "string",
              "null"
            ]
          }
        },
        "additionalProperties": false
      }
    },
    {
      "name": "get_brand_detail",
      "description": "Get detailed information for a specific brand by its ID",
      "inputSchema": {
        "type": "object",
        "$schema": "http://json-schema.org/draft-07/schema#",
        "required": [
          "brand_id"
        ],
        "properties": {
          "brand_id": {
            "type": "string"
          }
        },
        "additionalProperties": false
      }
    }
  ],
