      "description": "Create a Recipe Page on Instacart + return shareable URL. Attempts prod first then falls back to dev; errors from prod bubble up if both fail.",
      "inputSchema": {
        "type": "object",
        "title": "CreateRecipePageRequest",
        "required": [
          "title",
          "ingredients"
        ],
        "properties": {
          "title": {
            "type": "string",
            "title": "Title",
            "description": "Title of the recipe"
          },
          "image_url": {
            "type": "string",
            "title": "Image Url",
            "default": null,
            "nullable": true,
            "description": "Optional image for the recipe"
          },
          "expires_in": {
            "type": "integer",
            "title": "Expires In",
            "default": null,
            "maximum": 365,
            "minimum": 1,
            "nullable": true,
            "description": "Optional days until link expiration (1-365)"
          },
          "ingredients": {
            "type": "array",
            "items": {
              "type": "object",
              "title": "Ingredient",
              "required": [
                "name"
              ],
              "properties": {
                "name": {
                  "type": "string",
                  "title": "Name",
                  "description": "Ingredient name"
                },
                "unit": {
                  "type": "string",
                  "title": "Unit",
                  "default": null,
                  "nullable": true,
                  "description": "Unit of measure (e.g., each, lb, oz, g, ml)"
                },
                "quantity": {
                  "type": "number",
                  "title": "Quantity",
                  "default": null,
                  "nullable": true,
                  "description": "Requested quantity for the ingredient"
                }
              }
            },
            "title": "Ingredients",
            "description": "Recipe ingredients"
          },
          "instructions": {
            "type": "array",
            "items": {
              "type": "string",
              "properties": {}
            },
            "title": "Instructions",
            "default": null,
            "nullable": true,
            "description": "Optional list of instructions/steps"
          }
        }
      }
    },
