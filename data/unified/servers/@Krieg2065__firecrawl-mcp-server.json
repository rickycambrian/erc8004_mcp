          },
          "ignoreSitemap": {
            "type": "boolean",
            "description": "Skip sitemap.xml discovery and only use HTML links"
          },
          "includeSubdomains": {
            "type": "boolean",
            "description": "Include URLs from subdomains in results"
          }
        }
      }
    },
