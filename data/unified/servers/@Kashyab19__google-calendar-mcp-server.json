          "start_time": {
            "type": "string",
            "description": "Start time (RFC3339 format)"
          },
          "calendar_id": {
            "type": "string",
            "default": "primary",
            "description": "Calendar ID (default: 'primary')"
          },
          "description": {
            "type": "string",
            "description": "Event description"
          },
          "duration_minutes": {
            "type": "number",
            "description": "Duration in minutes (for relative events)"
          },
          "start_offset_minutes": {
            "type": "number",
            "description": "Minutes from now to start (0 = start now)"
          }
        },
        "additionalProperties": false
