                "start_time",
                "end_time"
              ],
              "properties": {
                "summary": {
                  "type": "string"
                },
                "end_time": {
                  "type": "string"
                },
                "location": {
                  "type": "string"
                },
                "attendees": {
                  "type": "array",
                  "items": {
                    "type": "string"
                  }
                },
                "start_time": {
                  "type": "string"
                },
                "description": {
                  "type": "string"
                }
              },
              "additionalProperties": false
            },
            "description": "Array of events to create (for batch creation)"
          },
          "all_day": {
            "type": "boolean",
            "default": false,
            "description": "Whether this is an all-day event"
          },
          "summary": {
            "type": "string",
            "description": "Event title (required for single event)"
          },
          "end_time": {
            "type": "string",
            "description": "End time (RFC3339 format)"
          },
          "location": {
            "type": "string",
            "description": "Event location"
          },
          "attendees": {
            "type": "array",
            "items": {
              "type": "string"
            },
            "description": "List of attendee email addresses"
          },
          "recurrence": {
            "type": "array",
            "items": {
              "type": "string"
            },
            "description": "RRULE patterns for recurring events (e.g., [\"FREQ=WEEKLY;BYDAY=MO,WE,FR\"])"
