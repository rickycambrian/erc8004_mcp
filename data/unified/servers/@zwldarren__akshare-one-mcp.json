                    "ADOSC",
                    "OBV",
                    "MOM",
                    "SAR",
                    "TSF",
                    "APO",
                    "AROON",
                    "AROONOSC",
                    "BOP",
                    "CMO",
                    "DX",
                    "MFI",
                    "MINUS_DI",
                    "MINUS_DM",
                    "PLUS_DI",
                    "PLUS_DM",
                    "PPO",
                    "ROC",
                    "ROCP",
                    "ROCR",
                    "ROCR100",
                    "TRIX",
                    "ULTOSC"
                  ],
                  "type": "string"
                }
              },
              {
                "type": "null"
              }
            ],
            "default": null,
            "description": "Technical indicators to add"
          },
          "interval_multiplier": {
            "type": "integer",
            "default": 1,
            "minimum": 1,
            "description": "Interval multiplier"
          }
        }
      },
      "outputSchema": {
