          "options": {
            "type": "array",
            "items": {
              "type": "string"
            },
            "description": "Additional test options"
          },
          "workingDir": {
            "type": "string",
            "description": "Working directory for the command"
          }
        },
        "additionalProperties": false
      }
    }
