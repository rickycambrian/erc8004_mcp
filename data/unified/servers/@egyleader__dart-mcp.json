            },
            "description": "Additional info options"
          }
        },
        "additionalProperties": false
      }
    },
    {
      "name": "dart-package",
      "inputSchema": {
        "type": "object",
        "$schema": "http://json-schema.org/draft-07/schema#",
        "required": [
          "command"
        ],
        "properties": {
          "args": {
            "type": "array",
            "items": {
              "type": "string"
            },
            "description": "Arguments for the pub subcommand"
          },
          "command": {
            "enum": [
              "get",
              "upgrade",
              "outdated",
              "add",
              "remove",
              "publish",
              "deps",
              "downgrade",
              "cache",
              "run",
              "global"
