        "type": "object",
        "$schema": "http://json-schema.org/draft-07/schema#",
        "required": [
          "script"
        ],
        "properties": {
          "args": {
            "type": "array",
            "items": {
              "type": "string"
            },
            "description": "Arguments to pass to the script"
          },
          "script": {
            "type": "string",
            "description": "Path to the Dart script to run"
          },
          "workingDir": {
            "type": "string",
            "description": "Working directory for the command"
          }
        },
        "additionalProperties": false
      }
    },
    {
      "name": "dart-test",
      "inputSchema": {
        "type": "object",
