  "name": "@dev/justcall-mcp",
  "display_name": "JustCall",
  "version": null,
  "description": "Manage calls, SMS, contacts, numbers, and campaigns from your JustCall workspace. Track agent, account, and dialer performance with robust analytics. Automate workflows with webhooks and streamline outreach with quick updates and real-time insights.",
  "icon_url": null,
