{
  "qualifiedName": "@concavegit/app-store-connect-mcp-server",
  "displayName": "app-store-connect-mcp-server",
  "description": "Manage apps, metadata, and localizations across App Store Connect. Streamline beta testing, user/device administration, and bundle ID capabilities. Access analytics and finance reports, and monitor CI workflows with build logs, issues, and test results.",
  "iconUrl": "https://icons.duckduckgo.com/ip3/github.com.ico",
  "remote": true,
  "deploymentUrl": "https://server.smithery.ai/@concavegit/app-store-connect-mcp-server",
  "connections": [
    {
      "type": "http",
      "deploymentUrl": "https://server.smithery.ai/@concavegit/app-store-connect-mcp-server/mcp",
      "configSchema": {
        "type": "object",
        "title": "MCP Session Configuration",
        "required": [
          "APP_STORE_CONNECT_KEY_ID",
          "APP_STORE_CONNECT_ISSUER_ID"
        ],
        "properties": {
          "APP_STORE_CONNECT_KEY_ID": {
            "type": "string",
            "minLength": 1,
            "description": "Your App Store Connect API Key ID (found in App Store Connect > Users and Access > Keys)"
          },
          "APP_STORE_CONNECT_P8_PATH": {
            "type": "string",
            "description": "Absolute path to your App Store Connect P8 private key file (provide either this or P8_B64_STRING)"
          },
          "APP_STORE_CONNECT_ISSUER_ID": {
            "type": "string",
            "minLength": 1,
            "description": "Your App Store Connect Issuer ID (found in App Store Connect > Users and Access > Keys)"
          },
          "APP_STORE_CONNECT_P8_B64_STRING": {
            "type": "string",
            "description": "Base64 encoded contents of your App Store Connect P8 private key file (provide either this or P8_PATH)"
          },
          "APP_STORE_CONNECT_VENDOR_NUMBER": {
            "type": "string",
            "description": "Your vendor number from App Store Connect (optional - enables sales and finance reporting tools)"
          }
        },
        "description": "Schema for the /mcp endpoint configuration",
        "x-query-style": "dot+bracket",
        "additionalProperties": false
      }
    }
  ],
  "security": null,
  "tools": [
