{
  "server": {
    "$schema": "https://static.modelcontextprotocol.io/schemas/2025-10-17/server.schema.json",
    "name": "io.github.ConnorBritain/mssql",
    "description": "MCP server for Microsoft SQL Server - schema discovery, profiling, and safe data operations",
    "repository": {
      "url": "https://github.com/ConnorBritain/mssql-mcp-server",
      "source": "github"
    },
    "version": "0.1.2",
    "packages": [
      {
        "registryType": "npm",
        "identifier": "@connorbritain/mssql-mcp-server",
        "version": "0.1.2",
        "transport": {
          "type": "stdio"
        },
        "environmentVariables": [
          {
            "description": "SQL Server hostname or IP address",
            "isRequired": true,
            "format": "string",
            "name": "SERVER_NAME"
          },
          {
            "description": "Target database name",
            "isRequired": true,
            "format": "string",
            "name": "DATABASE_NAME"
          },
          {
            "description": "Authentication mode: sql, windows, or aad",
            "format": "string",
            "name": "SQL_AUTH_MODE"
          },
          {
            "description": "Username for SQL or Windows auth",
            "format": "string",
            "name": "SQL_USERNAME"
          },
          {
            "description": "Password for SQL or Windows auth",
            "format": "string",
            "isSecret": true,
            "name": "SQL_PASSWORD"
          },
          {
            "description": "SQL Server port (default: 1433)",
            "format": "string",
            "name": "SQL_PORT"
          },
          {
            "description": "Set true for self-signed certs",
            "format": "string",
            "name": "TRUST_SERVER_CERTIFICATE"
          },
          {
            "description": "Set true to disable write operations",
            "format": "string",
            "name": "READONLY"
          }
        ]
      }
    ]
  },
